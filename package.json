{
  "name": "lighthouse-viewer-root",
  "version": "2.0.0",
  "description": "Root of the monorepo multipackage project for the lighthouse viewer stuffs",
  "main": "index.js",
  "scripts": {
    "lerna:start": "lerna run start --stream",
    "lerna:build": "pnpm run build --recursive",
    "lerna:release": "lerna version --conventional-commits --yes",
    "lerna:deploy:ci": "lerna publish from-package --yes",
    "lerna:deploy:ci:canary": "lerna publish from-package --yes --canary --preid beta --message 'chore: release new canary versions'",
    "lint": "eslint --quiet",
    "lint:fix": "pnpm lint -- --fix",
    "test": "snyk test && start-server-and-test demo:serve http://localhost:8080 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "snyk-protect": "snyk protect",
    "prepare": "pnpm snyk-protect",
    "demo:serve": "rimraf lighthouse-viewer && parcel index.html --port 8080 -d lighthouse-viewer --open",
    "demo:build": "rimraf lighthouse-viewer && parcel build index.html -d lighthouse-viewer --no-source-maps --public-url /lighthouse-viewer --no-autoinstall --no-minify",
    "demo:http-server": "http-server",
    "lerna:demo:build": "pnpm demo:build && pnpm run demo:build --recursive && pnpm run demo:copy --recursive"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-alias": "3.1.8",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-html": "0.2.4",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-replace": "3.0.0",
    "@rollup/plugin-run": "2.1.0",
    "@rollup/plugin-typescript": "8.3.0",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "cssnano": "5.0.12",
    "cypress": "9.1.1",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "http-server": "14.0.0",
    "lerna": "4.0.0",
    "parcel-bundler": "1.12.5",
    "parcel-plugin-svelte": "4.0.9",
    "postcss": "8.4.4",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "rollup": "2.61.0",
    "rollup-plugin-analyzer": "4.0.0",
    "rollup-plugin-dev": "2.0.1",
    "rollup-plugin-html": "0.2.1",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-terser": "7.0.2",
    "snyk": "1.787.0",
    "start-server-and-test": "1.14.0",
    "svelte": "3.44.2",
    "svelte-preprocess": "4.9.8",
    "tslib": "2.3.1",
    "typescript": "4.5.2"
  },
  "snyk": true,
  "browserslist": [
    "last 1 Chrome version"
  ],
  "volta": {
    "npm": "8.2.0",
    "node": "16.13.1"
  },
  "workspaces": [
    "packages/lighthouse-viewer",
    "packages/react2-lighthouse-viewer",
    "packages/svelte-lighthouse-viewer",
    "packages/vue-lighthouse-viewer",
    "packages/ngx-lighthouse-viewer"
  ]
}
