{
  "name": "viewer-root",
  "version": "1.0.0",
  "description": "Root of the monorepo multipackage project for the lighthouse viewer stuffs",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run lerna:link && npm run lerna:bootstrap",
    "lerna:start": "lerna run start --stream",
    "lerna:build": "lerna run build",
    "lerna:bootstrap": "lerna bootstrap --no-ci",
    "lerna:link": "lerna link",
    "lerna:release": "lerna version --conventional-commits --yes",
    "lerna:publish": "lerna publish from-git --yes",
    "lerna:publish:canary": "lerna publish from-git --canary --yes",
    "lint": "eslint --quiet",
    "lint:fix": "npm run lint -- --fix",
    "test": "start-server-and-test demo:serve http://localhost:8080 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "demo:serve": "rimraf demo && parcel index.html --port 8080 -d demo --open",
    "demo:build": "rimraf demo && parcel build index.html -d demo --public-url /lighthouse-viewer"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-run": "^2.0.2",
    "@rollup/plugin-typescript": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.14.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^6.2.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "lerna": "^3.22.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.36.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-dev": "^1.1.3",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "start-server-and-test": "^1.11.7",
    "svelte": "^3.31.2",
    "svelte-preprocess": "^4.6.1",
    "typescript": "^4.1.3"
  },
  "snyk": true,
  "dependencies": {
    "snyk": "^1.437.4"
  }
}
