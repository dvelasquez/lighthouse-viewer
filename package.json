{
  "name": "viewer-root",
  "version": "1.0.0",
  "description": "Root of the monorepo multipackage project for the lighthouse viewer stuffs",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run lerna:bootstrap",
    "lerna:start": "lerna run start --stream",
    "lerna:build": "lerna run build",
    "lerna:bootstrap": "lerna bootstrap && lerna link --force-local",
    "lerna:release": "lerna version --conventional-commits --yes",
    "lerna:deploy:ci": "lerna publish from-package --yes --message 'chore: release new versions'",
    "lerna:deploy:ci:canary": "lerna publish from-package --yes --canary --preid beta --message 'chore: release new canary versions'",
    "lint": "eslint --quiet",
    "lint:fix": "npm run lint -- --fix",
    "test": "snyk test && start-server-and-test demo:serve http://localhost:8080 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "demo:serve": "rimraf demo && parcel index.html --port 8080 -d demo --open",
    "demo:build": "rimraf demo && parcel build index.html -d demo --public-url /lighthouse-viewer"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-run": "^2.0.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.25.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^7.4.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "lerna": "^4.0.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-svelte": "^4.0.6",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.5",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-dev": "^1.1.3",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "start-server-and-test": "^1.12.3",
    "svelte": "^3.38.2",
    "svelte-preprocess": "^4.7.3",
    "typescript": "^4.3.2",
    "snyk": "^1.618.0"
  },
  "snyk": true
}
