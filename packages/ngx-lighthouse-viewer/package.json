{
  "name": "ngx-lighthouse-viewer-workspace",
  "version": "0.0.9",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "pnpm build:lib && pnpm build:demo",
    "build:lib": "ng build ngx-lighthouse-viewer && copyfiles ./README.md ./LICENSE ./dist/ngx-lighthouse-viewer",
    "build:demo": "ng build demo",
    "watch": "ng build --watch --configuration development",
    "demo:copy": "cpx \"dist/demo/**/*\" ../../lighthouse-viewer/ngx",
    "test": "ng test",
    "publish": "pnpm build:lib && pnpm publish ./dist/ngx-lighthouse-viewer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.2",
    "@angular/common": "~13.0.2",
    "@angular/compiler": "~13.0.2",
    "@angular/core": "~13.0.2",
    "@angular/forms": "~13.0.2",
    "@angular/platform-browser": "~13.0.2",
    "@angular/platform-browser-dynamic": "~13.0.2",
    "@angular/router": "~13.0.2",
    "lighthouse-viewer": "^0.1.26",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.2",
    "@types/jasmine": "~3.10.2",
    "@types/node": "^16.11.9",
    "concurrently": "^6.4.0",
    "copyfiles": "^2.4.1",
    "cssnano": "5",
    "jasmine-core": "~3.10.1",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.6",
    "typescript": "4.4.4"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
